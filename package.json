{
  "name": "edacation",
  "displayName": "EDAcation",
  "description": "Learning environment for digital hardware design with Yosys and nextpnr.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:edacation.helloWorld"
  ],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "edacation.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "edacation-container",
          "title": "EDAcation",
          "icon": "resources/icons/edacation.svg"
        }
      ]
    },
    "views": {
      "edacation-container": [
        {
          "id": "edacation-files",
          "name": "Files"
        }
      ]
    }
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/mocha": "^10.0.0",
    "eslint": "^8.26.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-web": "^0.0.32",
    "ts-loader": "^9.4.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@types/webpack-env": "^1.18.0",
    "assert": "^2.0.0",
    "process": "^0.11.10"
  }
}
