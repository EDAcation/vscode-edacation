{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2020",
        "outDir": "dist",
        "lib": ["ES2020", "WebWorker"],
        "esModuleInterop": true,
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    }
}
